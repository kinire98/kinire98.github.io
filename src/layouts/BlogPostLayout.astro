---
import MenuLayout from "./MenuLayout.astro";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<MenuLayout title={title}>
    <div id="central">
        <slot />
    </div>
</MenuLayout>

<style>
    #central {
        min-height: 100vh;
        min-width: 60vw;
        max-width: 75vw;
        padding: 100px;
        flex-direction: column;
        text-align: left;
        color: var(--white);
    }
</style>